$:.unshift File.join(File.dirname(__FILE__), "../../lib") 

require 'relisp'

slave = Relisp::RubySlave.new

def sample_ruby_method1
  elisp_eval("(+ 1 5)")
end

def sample_ruby_method2
  elisp_eval('(ruby-eval "(1 + 5)")')
end   

def sample_ruby_method3
  Relisp::Buffer.new("ruby-created-buffer")
end

slave.start
